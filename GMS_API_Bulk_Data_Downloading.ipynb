        "import pandas as pd\n",
        "from geopy.distance import geodesic\n",
        "\n",
        "def generate_coordinates(origin_lat, origin_lon, distance_meters, num_points_x, num_points_y):\n",
        "    \"\"\"Generates coordinates at a specified distance from an origin point.\n",
        "\n",
        "    Args:\n",
        "        origin_lat: Latitude of the origin point.\n",
        "        origin_lon: Longitude of the origin point.\n",
        "        distance_meters: Distance in meters.\n",
        "        num_points_x: Number of points along the x-axis (right direction).\n",
        "        num_points_y: Number of points along the y-axis (down direction).\n",
        "\n",
        "    Returns:\n",
        "        pandas.DataFrame: Coordinates as a DataFrame.\n",
        "    \"\"\"\n",
        "\n",
        "    coordinates = []\n",
        "    origin = (origin_lat, origin_lon)\n",
        "\n",
        "    for i in range(num_points_x):\n",
        "        for j in range(num_points_y):\n",
        "            # Calculate the distance based on index (i,j)\n",
        "            x_distance = distance_meters * i\n",
        "            y_distance = distance_meters * j\n",
        "\n",
        "            # Calculate the bearing (angle from north)\n",
        "            # Bearing 90 is right, 180 is down\n",
        "            destination_right = geodesic(meters=x_distance).destination(origin, bearing=90)\n",
        "            new_point_right = (destination_right.latitude, destination_right.longitude)\n",
        "\n",
        "            final_destination = geodesic(meters=y_distance).destination(new_point_right, bearing=180)\n",
        "            new_point = (final_destination.latitude, final_destination.longitude)\n",
        "\n",
        "            coordinates.append(new_point)\n",
        "\n",
        "    df = pd.DataFrame(coordinates, columns=[\"latitude\", \"longitude\"])\n",
        "    return df\n",
        "\n",
        "\n",
        "# Example Usage\n",
        "origin_latitude, origin_longitude = 29.980380737,73.403962547  # Example latitude\n",
        "distance = 1800  # Distance in meters\n",
        "num_x = 3  # Number of points in right direction\n",
        "num_y = 2 # Number of points in downwards direction\n",
        "\n",
        "coordinate_df = generate_coordinates(origin_latitude, origin_longitude, distance, num_x, num_y)\n",
        "\n",
        "# Save the DataFrame to an Excel file in your Google Drive\n",
        "coordinate_df.to_excel(\"/content/coordinates_check.xlsx\", index=False)  # Replace with your desired path\n"
      ]
