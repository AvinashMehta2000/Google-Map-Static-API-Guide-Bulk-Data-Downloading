    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "import os\n",
        "\n",
        "# Function to download satellite image\n",
        "def download_satellite_image(lat, lng, zoom=15, scale=2, image_size=(1280, 1280), api_key=\"YOUR_API_KEY\"):\n",
        "    \"\"\"\n",
        "    Downloads a satellite view image using the Google Maps Static API.\n",
        "\n",
        "    Parameters:\n",
        "        lat (float): Latitude of the center point.\n",
        "        lng (float): Longitude of the center point.\n",
        "        zoom (int): Zoom level of the map.\n",
        "        scale (int): Scale of the image (1 or 2 for high resolution).\n",
        "        image_size (tuple): Tuple of image width and height in pixels (max 640x640 for free tier).\n",
        "        api_key (str): Your Google Maps Static API key.\n",
        "\n",
        "    Returns:\n",
        "        None. Saves the image locally.\n",
        "    \"\"\"\n",
        "    base_url = \"https://maps.googleapis.com/maps/api/staticmap\"\n",
        "\n",
        "    # Construct the parameters for the API call\n",
        "    params = {\n",
        "        \"center\": f\"{lat},{lng}\",\n",
        "        \"zoom\": zoom,\n",
        "        \"size\": f\"{image_size[0]}x{image_size[1]}\",\n",
        "        \"scale\": scale,\n",
        "        \"maptype\": \"satellite\",\n",
        "        \"key\": api_key\n",
        "    }\n",
        "\n",
        "    # Make the API request\n",
        "    response = requests.get(base_url, params=params, stream=True)\n",
        "\n",
        "    # Check if the response is successful\n",
        "    if response.status_code == 200:\n",
        "        # Create a folder to save the images if it doesn't exist\n",
        "        if not os.path.exists(\"satellite_images\"):\n",
        "            os.makedirs(\"satellite_images\")\n",
        "\n",
        "        # Save the image\n",
        "        image_path = f\"satellite_images/satellite_{lat}_{lng}_zoom{zoom}_scale{scale}_{image_size}px.png\"\n",
        "        with open(image_path, \"wb\") as file:\n",
        "            for chunk in response.iter_content(1024):\n",
        "                file.write(chunk)\n",
        "\n",
        "        print(f\"Image successfully downloaded: {image_path}\")\n",
        "    else:\n",
        "        print(f\"Failed to download image. Status code: {response.status_code}, Error: {response.text}\")\n",
        "\n",
        "# Example usage\n",
        "if __name__ == \"__main__\":\n",
        "    # Replace with your latitude and longitude\n",
        "    latitude, longitude = 24.69453,75.56409\n",
        "\n",
        "    # Replace with your actual API key\n",
        "    api_key = \"YOUR_API_KEY\"\n",
        "\n",
        "    # Download the satellite image\n",
        "    download_satellite_image(latitude, longitude, zoom=15, scale=2, api_key=api_key)\n"
      ],
