        "import os\n",
        "from PIL import Image\n",
        "\n",
        "def extract_square_images(image_path, output_dir):\n",
        "    \"\"\"\n",
        "    Extracts four 640x640 square images from a 1280x1280 image.\n",
        "\n",
        "    Args:\n",
        "        image_path: Path to the input 1280x1280 image.\n",
        "        output_dir: Directory to save the extracted images.\n",
        "    \"\"\"\n",
        "    try:\n",
        "        img = Image.open(image_path)\n",
        "        width, height = img.size\n",
        "\n",
        "        if width != 1280 or height != 1280:\n",
        "            print(f\"Error: Input image {image_path} is not 1280x1280 pixels.\")\n",
        "            return\n",
        "\n",
        "        # Define the coordinates for the four squares\n",
        "        squares = [\n",
        "            (0, 0, 640, 640),\n",
        "            (640, 0, 1280, 640),\n",
        "            (0, 640, 640, 1280),\n",
        "            (640, 640, 1280, 1280),\n",
        "        ]\n",
        "\n",
        "        # Extract and save the squares\n",
        "        for i, (left, top, right, bottom) in enumerate(squares):\n",
        "            cropped_img = img.crop((left, top, right, bottom))\n",
        "            output_path = os.path.join(output_dir, f\"{i+1}.png\")\n",
        "            cropped_img.save(output_path)\n",
        "            print(f\"Saved cropped image {i+1} to {output_path}\")\n",
        "\n",
        "    except FileNotFoundError:\n",
        "        print(f\"Error: Image file not found at {image_path}\")\n",
        "    except Exception as e:\n",
        "        print(f\"An error occurred: {e}\")\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    # Example usage:\n",
        "    input_image_path = \"satellite_images/satellite_28.664371_76.69537_zoom15_scale2_(1280, 1280)px.png\" # Replace with your image path\n",
        "    output_directory = \"cropped_images\"\n",
        "    os.makedirs(output_directory, exist_ok=True)\n",
        "\n",
        "    extract_square_images(input_image_path, output_directory)"
      ],
