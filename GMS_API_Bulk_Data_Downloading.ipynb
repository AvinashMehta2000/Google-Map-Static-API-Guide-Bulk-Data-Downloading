      "source": [
        "import pandas as pd\n",
        "import folium\n",
        "\n",
        "# Replace 'your_file.xlsx' with the actual path to your Excel file in Google Drive\n",
        "file_path = \"/content/coordinates_check.xlsx\"\n",
        "\n",
        "# Read the Excel file into a pandas DataFrame\n",
        "try:\n",
        "    df = pd.read_excel(file_path)\n",
        "except FileNotFoundError:\n",
        "    print(f\"Error: File not found at {file_path}\")\n",
        "    exit()\n",
        "except Exception as e:\n",
        "    print(f\"An error occurred: {e}\")\n",
        "    exit()\n",
        "\n",
        "# Create a folium map centered around the first coordinate\n",
        "m = folium.Map(location=[df['latitude'][0], df['longitude'][0]], zoom_start=15)\n",
        "\n",
        "# Add markers for each coordinate\n",
        "for index, row in df.iterrows():\n",
        "    folium.Marker([row['latitude'], row['longitude']]).add_to(m)\n",
        "\n",
        "# Add a tile layer for satellite view\n",
        "folium.TileLayer('https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}', attr='Esri', name='Satellite').add_to(m)\n",
        "folium.LayerControl().add_to(m)\n",
        "\n",
        "# Display the map\n",
        "m"
